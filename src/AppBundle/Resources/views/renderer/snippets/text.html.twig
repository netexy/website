{% set propertyName = propertyName|default('blocks') %}

{% set customClass = '' %}
{% if block.custom_class is defined  %}
    {% set customClass = block.custom_class|default('') %}
{% endif %}

{% if block.content|length %}
    <div class="row">
        <div class="col-xs-12 pad-xs {{ customClass }}">
            {{ block.content|raw }}
        </div>
    </div>
{% endif %}
